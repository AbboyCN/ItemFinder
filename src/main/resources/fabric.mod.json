		"main": [
			"com.abboycn.itemfinder.ItemFinder"
		],
		"client": [
			"com.abboycn.itemfinder.ItemFinderClient"
		],
		"fabric-datagen": [
			"com.abboycn.itemfinder.ItemFinderDataGenerator"
		]
	},
	"mixins": [
		"itemfinder.mixins.json",
