{
	"schemaVersion": 1,
	"id": "itemfinder",
	"version": "${version}",
	"name": "ItemFinder",
	"description": "不要再在全物品里迷路啦~(TTS专用)",
	"authors": [
		"Ab_boy_CN"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/FabricMC/fabric-example-mod"
	},
	"license": "MIT",
	"icon": "assets/itemfinder/icon.png",
	"environment": "*",
